        </groups>
      <classes>
          <class name="com.quikr.website.cars.carsHeader.CarsHeaderTests"></class>
          <class name="com.quikr.website.cars.carsPAP.CarsPAPTests"></class>
          <class name="com.quikr.website.cars.carsSNB.CarsSNBTests"></class>
          <class name="com.quikr.website.cars.carsVAP.CarsVAPTests"></class>
          <class name="com.quikr.website.cars.carsSNB.NewCarsSNBTests"></class>
          <class name="com.quikr.website.cars.carsVAP.NewCarsVAPTests"></class>

      </classes>

